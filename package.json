    "mrs-developer": "^2.1.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "sass": "^1.63.4",
    "svelte": "^4.2.1",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
